  "name": "Git",
  "officialUrl": "https://git-scm.com/",
  "help": "Git is a <a href=\"https://git-scm.com/about/free-and-open-source\">free and open source</a> distributed version control system designed to handle everything from small to very large projects with speed and efficiency. Git is <a href=\"https://git-scm.com/documentation\">easy to learn</a> and has a <a href=\"https://git-scm.com/about/small-and-fast\">tiny footprint with lightning fast performance</a>. It outclasses SCM tools like Subversion, CVS, Perforce, and ClearCase with features like <a href=\"https://git-scm.com/about/branching-and-merging\">cheap local branching</a>, convenient <a href=\"https://git-scm.com/about/staging-area\">staging areas</a>, and <a href=\"https://git-scm.com/about/distributed\">multiple workflows</a>.",
  "pathExecutable": "git",
  "tasks": [
    {
      "customStart": true,
      "settingsClass": {
        "properties": [
          {
            "name": "Arguments",
            "type": "string",
            "format": "{value}",
            "createOverload": true
          }
        ]
      }
    }
  ]
